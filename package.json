{
  "name": "nestjs-twilio",
  "version": "2.0.0",
  "description": "Injectable Twilio client for Nestjs",
  "license": "MIT",
  "author": {
    "name": "Lazar Karic",
    "email": "kadzajr@gmail.com",
    "url": "https://lkaric.tech"
  },
  "repository": "https://github.com/rejvban/nestjs-twilio",
  "bugs": "https://github.com/rejvban/nestjs-twilio/issues",
  "private": false,
  "keywords": [
    "nestjs",
    "twilio",
    "sms",
    "client",
    "sdk",
    "typescript",
    "nestjs-twilio",
    "twilio-nestjs"
  ],
  "source": "lib/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:dist": "yarn clean:dist && tsc -p tsconfig.build.json",
    "test:run": "NODE_ENV=test jest",
    "test:watch": "yarn test:run --watch",
    "test:coverage": "yarn clean:test && yarn test:run --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint --fix . --ext .js,.ts,.tsx",
    "clean:dist": "rimraf dist",
    "clean:test": "rimraf coverage",
    "prepare": "yarn build:dist",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@nestjs/common": "8.0.6",
    "@nestjs/core": "8.0.6",
    "@nestjs/testing": "8.0.6",
    "@types/jest": "27.0.1",
    "@types/node": "16.7.1",
    "@types/twilio": "^3.19.3",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "27.0.6",
    "lint-staged": "^11.1.2",
    "pinst": "^2.1.6",
    "prettier": "2.3.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "7.3.0",
    "ts-jest": "27.0.5",
    "twilio": "3.67.0",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "twilio": "^3.63.1"
  }
}
