{
  "name": "nestjs-twilio",
  "version": "2.1.0",
  "description": "Injectable Twilio client for Nestjs",
  "license": "MIT",
  "author": {
    "name": "Lazar Karic",
    "email": "kadzajr@gmail.com",
    "url": "https://lkaric.tech"
  },
  "repository": "https://github.com/rejvban/nestjs-twilio",
  "bugs": "https://github.com/rejvban/nestjs-twilio/issues",
  "private": false,
  "keywords": [
    "nestjs",
    "twilio",
    "sms",
    "client",
    "sdk",
    "typescript",
    "nestjs-twilio",
    "twilio-nestjs"
  ],
  "source": "lib/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:dist": "yarn clean:dist && tsc -p tsconfig.build.json",
    "test:run": "NODE_ENV=test jest",
    "test:watch": "yarn test:run --watch",
    "test:coverage": "yarn clean:test && yarn test:run --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint --fix . --ext .js,.ts,.tsx",
    "clean:dist": "rimraf dist",
    "clean:test": "rimraf coverage",
    "prepare": "yarn build:dist",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@nestjs/common": "8.4.4",
    "@nestjs/core": "8.4.4",
    "@nestjs/testing": "8.4.4",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "@types/twilio": "^3.19.3",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "27.5.1",
    "lint-staged": "^12.3.7",
    "pinst": "^3.0.0",
    "prettier": "2.6.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "7.5.5",
    "ts-jest": "27.1.4",
    "twilio": "3.76.1",
    "typescript": "4.6.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "twilio": "^3.63.1"
  }
}
